@{
    // Used Table [Member]
    // 고객관리시스템 - 고객목록
    // 선행작업 - 관리자 로그인
    Layout = "~/Views/Shared/_LayoutKi4C_Aside_dg.cshtml";
    ViewData["Title"] = "기준정보 관리/진단 종류";
}
<!-- 섹션 -->
<section>
    <!-- 콘텐츠 -->
    <div id=contetns style="width: 100%;">
        <h2>진단 구분 등록</h2>
        <br />
        <div id="menuIdx">
            <div style="float: left;">
                <img id=home_img src="~/images/home-1.png">
            </div>
            <a href="#">Home</a>
            &gt;
            <a href="#">기준 정보</a>
            &gt;
            진단 구분 등록
        </div>
        <br>
        <hr style="border: solid 0.5px black; width: 100%;" />
        <!-- 진단 구분 등록 -->
        <div style="padding-top: 30px; padding-left: 30px; height: 200px;">
            <a>진단종류선택: </a>
            <form style="padding-top: 10px; display: inline-block;">
                <select style="max-width: 400px; width: 130%; height: 30px; font-size: 16px;
                        margin-bottom: 3px;">
                    <option>=== 선택 ===</option>
                    <option>기업 기초 정보</option>
                    <option>자체 진단</option>
                    <option>전문가 진단</option>
                    <option>기타</option>
                </select>
            </form>
            <br>
            <a>진단구분코드: </a>
            <form style="padding-top: 10px; display: inline-block;">
                <input type="text"
                       style="max-width: 400px; width: 130%; height: 25px; font-size: 16px;
                        margin-bottom: 3px;" />
            </form>
            <br>
            <a>진단구분명: </a>
            <form style="padding-top: 10px; padding-left: 14px; display: inline-block;">
                <input type="text"
                       style="max-width: 400px; width: 130%; height: 25px; font-size: 16px;
                        margin-bottom: 3px;" />
            </form>
            <div style="padding-top: 30px; padding-left: 50px;">
                <input id="registButton" type="submit" value="등록" />
                <input id="modifyButton" type="submit" value="수정" />
            </div>
        </div>
        <!-- 진단 구분 등록 -->
        <hr />
        <!-- 진단 구분 목록 -->
        <div style="display: inline-block;">
            <div id="buttonLocation">
                <input type="text" id="searchBox" placeholder="검색어 입력" />
                <button id="searchButton">검색</button>
            </div>
            <br>
            <div style="width: 90%; margin: auto;">
                <table id="table">
                    <tr align="center" bgcolor="skyblue">
                        <td>번호</td>
                        <td>진단구분코드</td>
                        <td>진단구분명</td>
                        <td>삭제</td>
                    </tr>
                    <tr align="center" bgcolor="white">
                        <td>1</td>
                        <td>B1</td>
                        <td>디지털 전략</td>
                        <td><input type="checkbox"></td>
                    </tr>
                    <tr align="center" bgcolor="white">
                        <td>2</td>
                        <td>B2</td>
                        <td>디지털 비즈니스</td>
                        <td><input type="checkbox"></td>
                    </tr>
                    <tr align="center" bgcolor="white">
                        <td>3</td>
                        <td>B3</td>
                        <td>디지털 오퍼레이션</td>
                        <td><input type="checkbox"></td>
                    </tr>
                    <tr align="center" bgcolor="white">
                        <td>4</td>
                        <td>B4</td>
                        <td>디지털 데이터</td>
                        <td><input type="checkbox"></td>
                        </td>
                    </tr>
                    <tr align="center" bgcolor="white">
                        <td>5</td>
                        <td>B5</td>
                        <td>디지털 조직문화</td>
                        <td><input type="checkbox"></td>
                        </td>
                    </tr>
                </table>
            </div>
            <div id="buttonLocation">
                <input id="deleteButton" type="submit" value="항목 삭제" />
            </div>
        </div>
        <!-- 진단 구분 목록 -->
        <!-- 콘텐츠 -->
</section>
<!-- 섹션 -->